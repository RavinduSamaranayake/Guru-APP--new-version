        android:theme="@style/AppTheme">
        <activity android:name=".splashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Login" />
        <activity android:name=".Menu" />
        <activity android:name=".MainList" />
        <activity android:name=".Auth" />
        <activity android:name=".Institutes" />
        <activity android:name=".classes" />
        <activity android:name=".userRegister" />
        <activity android:name=".StudentRegistration" />
        <activity android:name=".ChooseInstitue" />
        <activity android:name=".WaitingReg" />
        <activity android:name=".teacherRegistration" />
        <activity
            android:name=".Home"
            android:label="@string/title_activity_home"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SettingsActivity"
            android:label="@string/title_activity_settings" />
        <activity android:name=".about" />
        <activity android:name=".Help" />

        <meta-data
            android:name="com.google.android.actions"
            android:resource="@xml/cercleshape" />

        <activity android:name=".Models.Settings" />
        <activity android:name=".Settings" />
        <activity android:name=".rateInstitute"></activity>
